{
  "env": {
    "browser": true,
    "es2021": true
  },
  "root": true,
  "extends": [
    "standard-with-typescript",
    "prettier",
    "plugin:prettier/recommended"
  ],
  //airbnb-base
  "plugins": ["prettier"],

  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "tsconfig.json"
  },
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
